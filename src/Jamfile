SubDir TOP src ;


Main zmbvc$(BIN_SFX) : test.c ;
LinkLibraries zmbvc$(BIN_SFX) : libzmbv.a ;
if $(USE_ZLIB) { LINKFLAGS on zmbvc$(BIN_SFX) += -lz ; }

Main zmbv_unpack$(BIN_SFX) : unpack.c ;
LinkLibraries zmbv_unpack$(BIN_SFX) : libzmbv.a ;
if $(USE_ZLIB) { LINKFLAGS on zmbv_unpack$(BIN_SFX) += -lz ; }

Main zmbv_unpack_small$(BIN_SFX) : unpack_small.c ;
LinkLibraries zmbv_unpack_small$(BIN_SFX) : libzmbvu.a ;
if $(USE_ZLIB) { LINKFLAGS on zmbv_unpack_small$(BIN_SFX) += -lz ; }


SubInclude TOP src libzmbv ;
SubInclude TOP src libzmbvu ;
